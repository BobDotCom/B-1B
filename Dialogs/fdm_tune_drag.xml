                    <halign>left</halign>
                    <live>true</live>

                    <pref-width>100</pref-width>
                    <label>Drag_basic</label>
                </text>

                <text>
                    <halign>left</halign>
                    <label>-9.9999</label>
                    <pref-width>80</pref-width>
                    <format>%-0.4f</format>
                    <property>/fdm/jsbsim/aero/force/Drag_basic</property>
                    <live>true</live>
                </text>

                <slider>
                    <vertical>false</vertical>
                    <pref-width>500</pref-width>
                    <fraction>0</fraction>
                    <property>/fdm/jsbsim/tune/drag/basic</property>
                    <min>-2</min>
                    <max>2</max>
                    <binding>
                        <command>dialog-apply</command>
                    </binding>
                </slider>
                <text>
                    <halign>right</halign>
                    <label>-9.9999</label>
                    <format>%-0.4f</format>
                    <property>/fdm/jsbsim/tune/drag/basic</property>
                    <live>true</live>
