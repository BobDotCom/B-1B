<PropertyList>
    <name>fdm_tune_drag</name>
    <modal>false</modal>
    <layout>vbox</layout>
    <group>
        <layout>hbox</layout>
        <text>
            <halign>center</halign>
            <label>Tune Drag</label>
        </text>
    </group>
