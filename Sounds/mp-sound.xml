<?xml version="1.0"?>

<PropertyList>

<fx>

<!-- External Sounds -->
 
 <!-- Engines-->

       <engine>
         <name>airgen-ext</name>
         <path>Aircraft/Jaguar-GR1/Sounds/airgenint.wav</path>
         <mode>looped</mode>
         <condition>
            <greater-than>
              <property>engines/air-gen-n1</property>
              <value>1</value>
            </greater-than>
           <not>
             <property>/sim/current-view/internal</property>
           </not>
         </condition>
         <volume>
           <property>engines/air-gen-n1</property>
           <factor type="double">0.0075</factor>
           <offset>0.25</offset>
           <min>0</min>
           <max>2</max>
         </volume>
         <pitch>
           <property>engines/air-gen-n1</property>
           <factor>0.030</factor>
           <offset>0</offset>
           <min>0</min>
           <max>2</max>
         </pitch>
       </engine>

    <engine>
      <name>engine-start-outside</name>
      <mode>once</mode>
      <path>Aircraft/Jaguar-GR1/Sounds/startup.wav</path>
      <condition>
        <and> 
       <greater-than>  
         <property>engines/engine[0]/n1</property>
           <value>0.5</value>   
       </greater-than>
            <less-than>
                <property>velocities/groundspeed-kt</property>
                <value>1</value>
            </less-than>
              <not>
         <property>/sim/current-view/internal</property>
              </not>
          </and>
        </condition>
      <volume>
        <property>engines/engine[0]/n2</property>
        <factor> 0.09 </factor>
        <min> 0.3 </min>
        <max> 0.9 </max>
      </volume>
      <pitch>
        <property>engines/engine[0]/n2</property>
        <factor> 0.01 </factor>
        	<min> 0.0 </min>
        	<max> 0.9 </max>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>3000</max-dist>
    </engine>
	
  	<engine>
      <name>engine-start-outside1</name>
      <mode>once</mode>
      <path>Aircraft/Jaguar-GR1/Sounds/startup.wav</path>
      <condition>
        <and> 
      <greater-than>  
         <property>engines/engine[1]/rpm</property>
           <value>0.5</value>
     </greater-than>
            <less-than>
                <property>velocities/groundspeed-kt</property>
                <value>1</value>
            </less-than>
            <not>
          <property>/sim/current-view/internal</property>
            </not>
          </and>
      </condition>
      <volume>
        <property>engines/engine[1]/n2</property>
        <factor> 0.09 </factor>
        <min> 0.3 </min>
        <max> 0.9 </max>
      </volume>
      <pitch>
        <property>engines/engine[1]/n2</property>
        <factor> 0.01 </factor>
        	<min> 0.0 </min>
        	<max> 0.9 </max>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>3000</max-dist>
    </engine>
	
  	<engine>
      <name>engine</name>
      <path>Aircraft/Jaguar-GR1/Sounds/jet-blast.wav</path>
      <mode>looped</mode>
      <condition>
        <property>engines/engine[0]/running</property>
    		<not>
        	<property>/sim/current-view/internal</property>
    		<property>engines/engine/out-of-fuel</property>
       	</not>
      </condition>
       <position>
          <x>18.6</x>
        	<y>-0.4</y>
        	<z>2.37</z>
       </position>
           <orientation>
            	<x>1.0</x>
              <y>0.0</y>
              <z>0</z>
            <inner-angle>35.0</inner-angle>
            <outer-angle>90</outer-angle>
            <outer-gain>0.45</outer-gain>
           </orientation>
        <volume>
        <property>engines/engine[0]/n1</property>
        <factor>0.014</factor>
        <offset>-0.45</offset>
        </volume>
        <reference-dist>20</reference-dist>
        <max-dist>6000</max-dist>
    </engine>
	
  	<engine>
      <name>engine1</name>
      <path>Aircraft/Jaguar-GR1/Sounds/jet-blast.wav</path>
      <mode>looped</mode>
      <condition>
        <property>engines/engine[1]/running</property>
	    	<not>
        	<property>/sim/current-view/internal</property>
	    	<property>engines/engine/out-of-fuel</property>
     	</not>
      </condition>
       <position>
          <x>18.6</x>
        	<y>0.4</y>
        	<z>2.37</z>
       </position>
       <orientation>
        	<x>1.0</x>
          <y>0.0</y>
          <z>0</z>
        <inner-angle>35.0</inner-angle>
        <outer-angle>90</outer-angle>
        <outer-gain>0.45</outer-gain>
       </orientation>
          <volume>
        <property>engines/engine[1]/n1</property>
          <factor>0.014</factor>
          <offset>-0.45</offset>
          </volume>
            <reference-dist>20</reference-dist>
            <max-dist>6000</max-dist>
      </engine>
  
  
      <engine>
         <name>Engine Exhaust</name>
         <mode>looped</mode>
           <path>Aircraft/Jaguar-GR1/Sounds/Jaguar-whine.wav</path>
           <condition>
    	      <property>/engines/engine/running</property>
    	         <not>
         	  <property>/sim/current-view/internal</property>
	         	<property>/engines/engine/out-of-fuel</property>
     	        </not>
            </condition>
           <position>
            	<x>6.0</x>
            	<y>0.0</y>
	            <z>0.0</z>
           </position>
             <orientation>
                	<x>90.0</x>
                	<y>0.0</y>
                	<z>0.0</z>
          	<inner-angle>80.0</inner-angle>
          	<outer-angle>110.0</outer-angle>
          	<outer-gain>0.5</outer-gain>
             </orientation>
           <volume>
          <property>engines/engine/n2</property>
            <factor>0.08</factor>
              <min>0.4</min>
            	<max>1.0</max>
           </volume>
           <pitch>
        	<property>engines/engine/n2</property>
            	<factor>0.005</factor>
	            <min>0</min>
	            <max>0.7</max>
            </pitch>
          <reference-dist>600.0</reference-dist>
              <max-dist>6000.0</max-dist>
        </engine>
  
   <engine>
      <name>thrust lowfreq exterior</name>
      <path>Aircraft/Jaguar-GR1/Sounds/engine-lowfreq.wav</path>
      <mode>looped</mode>
      <condition>
        <property>/engines/engine/running</property>
        <not>
          <property>/sim/current-view/internal</property>
        </not>
      </condition>
      <property>engines/engine/thrust_lb</property>
      <volume>
        <property>engines/engine/thrust_lb</property>
        <factor>0.008</factor>       
        <min>0</min>
        <max>4.9</max>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>
  
  <!-- Afterburner -->
  
   <engine>
      <name>burner lowfreq</name>
      <path>Aircraft/Jaguar-GR1/Sounds/burner-lowfreq.wav</path>
      <mode>once</mode>    
      <property>engines/engine/augmentation</property>
      <volume>
        <offset>4.9</offset>
      </volume> 
      <reference-dist>100</reference-dist>
      <max-dist>6000</max-dist>
    </engine>

    <engine>
      <name>burner lowfreq loop0</name>
      <path>Aircraft/Jaguar-GR1/Sounds/engine-lowfreq.wav</path>
      <mode>looped</mode>
      <property>engines/engine[0]/augmentation</property>
      <volume>
        <offset>-0.2</offset>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>6000</max-dist>
    </engine>
	
	 <engine>
      <name>burner lowfreq loop1</name>
      <path>Aircraft/Jaguar-GR1/Sounds/engine-lowfreq.wav</path>
      <mode>looped</mode>
      <property>engines/engine[1]/augmentation</property>
      <volume>
        <offset>-0.2</offset>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>6000</max-dist>
    </engine>
	
    <engine>
      <name>burner loop0</name>
      <path>Aircraft/Jaguar-GR1/Sounds/reheat.wav</path>
      <mode>looped</mode>
      <condition>
        <property>/engines/engine[0]/running</property>
        <not>
          <property>/sim/current-view/internal</property>
        </not>
      </condition>
      <volume>
        <property>engines/engine[0]/augmentation</property>
        <factor>2</factor>
        <min>0</min>
        <max>4.5</max>
      </volume>
      <orientation>
        <x>0.0</x>
        <y>0.0</y>
        <z>180.0</z>
        <inner-cone>180.0</inner-cone>
        <outer-cone>360.0</outer-cone>
        <outer-gain>0.2</outer-gain>
      </orientation>
      <reference-dist>100</reference-dist>
      <max-dist>7000</max-dist>
    </engine>
	
	   <engine>
      <name>burner loop1</name>
      <path>Aircraft/Jaguar-GR1/Sounds/reheat.wav</path>
      <mode>looped</mode>
      <condition>
        <property>/engines/engine[1]/running</property>
        <not>
          <property>/sim/current-view/internal</property>
        </not>
      </condition>
      <volume>
        <property>engines/engine[1]/augmentation</property>
        <factor>2</factor>
        <min>0</min>
        <max>4.5</max>
      </volume>
      <orientation>
        <x>0.0</x>
        <y>0.0</y>
        <z>180.0</z>
        <inner-cone>180.0</inner-cone>
        <outer-cone>360.0</outer-cone>
        <outer-gain>0.2</outer-gain>
      </orientation>
      <reference-dist>100</reference-dist>
      <max-dist>7000</max-dist>
    </engine>

    

    <!-- Sonic Boom -->
  <sonicboom>
   <name>Sonic Boom</name>
	<mode>once</mode>
   <path>Aircraft/Jaguar-GR1/Sounds/sonicboom.wav</path>
   <condition>
   <and>
	    <greater-than>
	     	<property>velocities/mach</property>
	     	<value>1.0</value>
	    </greater-than>
	    <equals>
		    <property>/sim/current-view/internal</property>
		    <value>false</value>
		</equals>
	</and>
   </condition>
    <reference-dist>600.0</reference-dist>
    <max-dist>9000.0</max-dist>
  </sonicboom>


 
  <!-- flyby -->
		<flyby>
			<name>fly-by</name>
			<mode>looped</mode>
			<path>Aircraft/Jaguar-GR1/Sounds/flyby.wav</path>
			<condition>
			<and>
	    <greater-than>
	     	<property>velocities/mach</property>
	     	<value>0.8</value>
	    </greater-than>
				<equals>
					<property>sim/current-view/view-number</property>
					<value>6</value>
				</equals>
				</and>
				<not>
         	<property>/sim/current-view/internal</property>
	 	<property>/engines/engine/out-of-fuel</property>
     	</not>
		</condition>
			 <volume>
				<property>velocities/airspeed-kt</property>
				<factor>0.001</factor>
			</volume>
		</flyby>


    <guns>
      <name>gunfire</name>
      <mode>looped</mode>
      <path>Aircraft/Jaguar-GR1/Sounds/gunfire.wav</path>
      <condition> 
        <property>controls/armament/trigger</property>
        <property>ai/submodels/submodel/count</property>
      </condition>
      <volume> 
        <factor>1.2</factor>
        <max>1.0</max>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
	    <reference-dist>600.0</reference-dist>
      <max-dist>2000.0</max-dist>
	   </guns>

	
	</fx>
</PropertyList>
