<?xml version="1.0"?>
<PropertyList>

 <path>strobe.ac</path>
 <offsets>
    <x-m> 0.0 </x-m>
    <y-m> 0.0 </y-m>
    <z-m> 0.0 </z-m>
 </offsets>
 
  <animation>
	 <type>timed</type>
	 <object-name>strobe</object-name>
	 <object-name>glow</object-name>
	 <use-personality type="bool">true</use-personality>
	 <branch-duration-sec>
		 <random>
			 <min>0.1</min>
			 <max>0.1</max>
		 </random>
	 </branch-duration-sec>
	 <branch-duration-sec>
		 <random>
			 <min>2.0</min>
			 <max>2.2</max>
		 </random>
	 </branch-duration-sec>
 </animation>
 
<animation>
	<type>billboard</type>
	<object-name>strobe</object-name>
	<spherical type="bool">true</spherical>
</animation>

<animation>
	 <type>billboard</type>
	 <object-name>glow</object-name>
	 <spherical type="bool">true</spherical>
</animation>
 

 <animation>
	 <type>alpha-test</type>
	 <object-name>strobe</object-name>
	 <alpha-factor>0.001</alpha-factor>
 </animation>
 
 <animation>
	 <type>select</type>
	 <object-name>strobe</object-name>
	 <object-name>glow</object-name>
	 <condition>
			 <equals>
				 <property>sim/multiplay/generic/int[0]</property>
				 <value>1.0</value>
			 </equals>
	 </condition>
  </animation>

</PropertyList>
