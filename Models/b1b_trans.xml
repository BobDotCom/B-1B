 <!-- rain -->
 
 <animation>
	 <type>select</type>
	 <object-name>bone_rain.001</object-name>
	 <condition>
	   <and>
	      <equals>
		      <property>environment/rain/above_clouds</property>
		      <value>0</value>
	      </equals>
	       <or>
		 <greater-than>
			 <property>environment/metar/rain-norm</property>
			 <value>0.2</value>
		 </greater-than>
		 <greater-than>
			 <property>environment/metar/snow-norm</property>
			 <value>0.2</value>
		 </greater-than>
	        </or>
	    </and>
	 </condition>
  </animation>
 
 <animation>
	 <type>textranslate</type>
	 <object-name>bone_rain.001</object-name>
	 <property>environment/rain/rain</property>
	 <factor>0.1</factor>
	 <step>1</step>
	 <axis>
		 <x>0</x>
		 <y>1</y>
		 <z>0</z>
	 </axis>
 </animation>
 
 
