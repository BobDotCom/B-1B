<?xml version="1.0"?>
<PropertyList> 

<model>
 <name>WhiteLightleft</name>
 <path>Aircraft/generic/spotlight.xml</path>
 <offsets>
  <x-m>3.5</x-m>
  <y-m>-21.2</y-m>
  <z-m>-1.0</z-m>
 </offsets>
 <overlay>
  <params>
   <name>WhiteLightleft</name>
   <power-source>controls/lighting/nav-lights</power-source>
   <direction>
    <x>0.5</x>
    <y>0.8</y>
    <z>0.0</z>
   </direction> 
   <ambient>
    <r>1.0</r>
    <g>1</g>
    <b>1</b>
   </ambient>
   <specular>
    <r>1.0</r>
    <g>1.0</g>
    <b>1.0</b>
   </specular> 
  </params>
 </overlay>
</model> 

<animation>
  <name>WhiteLightleft</name>  
  <type>scale</type>
  <x-offset>2.0</x-offset>
  <y-offset>2.0</y-offset>
  <z-offset>2.0</z-offset>
  <center>
  <x-m>3.5</x-m>
  <y-m>-21.2</y-m>
  <z-m>-1.0</z-m>
  </center> 
 </animation>


</PropertyList>
