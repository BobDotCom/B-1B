<?xml version="1.0"?>
<PropertyList>

 <path>landing_spot.ac</path>
 <offsets>
    <x-m> 0.0 </x-m>
    <y-m> 0.0 </y-m>
    <z-m> 0.0 </z-m>
 </offsets>


 

 <animation>
	 <type>alpha-test</type>
	 <object-name>spot_light1</object-name>
	 <alpha-factor>0.002</alpha-factor>
 </animation>
 <animation>
	 <type>select</type>
	 <object-name>spot_light1</object-name>
	 <condition>
		 <and>
			 <equals>
				 <property>controls/lighting/nav-lights</property>
				 <value>1.0</value>
			 </equals>
			 <equals>
			   <property>gear/gear[0]/position-norm</property>
			   <value>1.0</value>
			 </equals>
		 </and>
	 </condition>
</animation>


 

 <animation>
	 <type>alpha-test</type>
	 <object-name>spot_light2</object-name>
	 <alpha-factor>0.002</alpha-factor>
 </animation>
 <animation>
	 <type>select</type>
	 <object-name>spot_light2</object-name>
	 <condition>
		 <and>
			 <equals>
				 <property>controls/lighting/nav-lights</property>
				 <value>1.0</value>
			 </equals>
			 <equals>
			   <property>gear/gear[0]/position-norm</property>
			   <value>1.0</value>
			 </equals>
		 </and>
	 </condition>
</animation>

