<?xml version="1.0"?>
<!--
sweat by maggse
    -->
<PropertyList>

 <path>caution_ind.ac</path>




 <!-- eject switch -->

 <animation>
	 <type>rotate</type>
	 <object-name>switch_eject</object-name>
	 <property>controls/switches/eject</property>
	 <interpolation>
		 <entry>
			 <ind>0.0</ind>
			 <dep>30.0</dep>
		 </entry>
		 <entry>
			 <ind>1.0</ind>
			 <dep>-30.0</dep>
		 </entry>
	 </interpolation>
	 <axis>
		 <x>0.0</x>
		 <y>1.0</y>
		 <z>0.0</z>
	 </axis>
	 <center>
		 <x-m>0.005</x-m>
		 <y-m>-0.424</y-m>
		 <z-m>0.084</z-m>
	 </center>
 </animation>

 <!-- clickable buttons block (osg only!) xxxxxxxxxxxxxxxxxxxxxxxxxxxxxx-->

 <animation>
	 <type>pick</type>
	 <object-name>switch_eject</object-name>
	 <action>
		 <button>0</button>
		 <repeatable>true</repeatable>
		 <interval-sec>0.5</interval-sec>
		 <binding>
			 <command>property-toggle</command>
			 <property>controls/switches/eject</property>
		 </binding>
	 </action>
</animation>


<!-- caution ind -->

 <animation>
	 <type>translate</type>
	 <object-name>rep_mstrcaution</object-name>
	 <object-name>rep_eng1.001</object-name>
         <property>controls/switches/master_caution</property>
	 <interpolation>
		 <entry>
			 <ind>0.0</ind>
			 <dep>0.0</dep>
		 </entry>
		 <entry>
			 <ind>1.0</ind>
			 <dep>-0.00001</dep>
		 </entry>
	 </interpolation>
	 <axis>
		 <x>1.0</x>
		 <y>0.0</y>
		 <z>0.0</z>
	 </axis>
	 <center>
		 <x-m>0.01097</x-m>
		 <y-m>-0.28408</y-m>
		 <z-m>0.10661</z-m>
	 </center>

</animation>
 <animation>
	 <type>pick</type>
	 <object-name>rep_mstrcaution</object-name>
	 <action>
		 <button>0</button>
		 <repeatable>true</repeatable>
		 <interval-sec>0.5</interval-sec>
		 <binding>
			 <command>property-toggle</command>
			 <property>controls/switches/master_caution</property>
		 </binding>
	 </action>
</animation>

<animation>
	<type>select</type>
	<object-name>rep_mstrcaution</object-name> 
        <condition>  
        <or>
         <equals> 
          <property>controls/switches/master_caution</property>
          <value>1</value>
         </equals>
         <equals>
          <property>instrumentation/annunciators/preptoeject</property>
          <value>1</value> 
         </equals>
         <equals>
          <property>instrumentation/annunciators/flapwing</property>
          <value>1</value> 
         </equals>
        </or>
        </condition>
</animation>

<animation>
	<type>material</type>
	<object-name>rep_mstrcaution</object-name>
	 <emission>
		 <red>0.9</red>
		 <green>0.5</green>
		 <blue>0.9</blue>
		 <factor-prop>controls/switches/annunciators-norm</factor-prop>
	</emission>
</animation>


<animation>
	<type>select</type>
	<object-name>GLIDESLOPE</object-name> 
        <condition> 
         <or> 
         <equals> 
          <property>instrumentation/nav/gs-in-range</property>
          <value>1</value> 
         </equals>
         <equals> 
          <property>instrumentation/nav[1]/gs-in-range</property>
          <value>1</value> 
         </equals>
         </or>
        </condition>
</animation>
<animation>
	<type>material</type>
	<object-name>GLIDESLOPE</object-name>
	 <emission>
		 <red>0.9</red>
		 <green>0.5</green>
		 <blue>0.9</blue>
		 <factor-prop>controls/switches/annunciators-norm</factor-prop>
	</emission>
</animation>

<animation>
	<type>select</type>
	<object-name>REDUCESPEED</object-name> 
        <condition> 
         <equals>
          <property>instrumentation/annunciators/reducespeed</property>
          <value>1</value>
         </equals>
        </condition>
</animation>

<animation>
	<type>material</type>
	<object-name>REDUCESPEED</object-name>
	 <emission>
		 <red>0.9</red>
		 <green>0.5</green>
		 <blue>0.9</blue>
		 <factor-prop>controls/switches/annunciators-norm</factor-prop>
	</emission>
</animation>

<animation>
	<type>select</type>
	<object-name>STALL</object-name> 
        <condition> 
         <equals>
          <property>instrumentation/annunciators/stall</property>
          <value>1</value>
         </equals>
        </condition>
</animation>

<animation>
	<type>material</type>
	<object-name>STALL</object-name>
	 <emission>
		 <red>0.9</red>
		 <green>0.5</green>
		 <blue>0.9</blue>
		 <factor-prop>controls/switches/annunciators-norm</factor-prop>
	</emission>
</animation>

<animation>
	<type>material</type>
	<object-name>MINDECNHGT</object-name>
	 <emission>
		 <red>0.9</red>
		 <green>0.5</green>
		 <blue>0.9</blue>
		 <factor-prop>controls/switches/annunciators-norm</factor-prop>
	</emission>
</animation>

 <animation>
	 <type>select</type>
	 <object-name>PREPTOEJECT</object-name>
	 <condition>
		 <equals>
			 <property>instrumentation/annunciators/preptoeject</property>
			 <value>1.0</value>
		 </equals>
	 </condition>
 </animation>
 <animation>
	 <type>material</type>
	 <object-name>PREPTOEJECT</object-name>
	 <emission>
		 <red>0.5</red>
		 <green>0.9</green>
		 <blue>0.5</blue>
		 <factor-prop>controls/switches/annunciators-norm</factor-prop>
	 </emission>
</animation>

 <animation>
     <type>select</type>
      <object-name>flapwing</object-name> 
        <condition>   
         <equals>
          <property>instrumentation/annunciators/flapwing</property> 
          <value>1</value>
         </equals>
        </condition> 
</animation>
 <animation>
	 <type>material</type>
	 <object-name>flapwing</object-name>
	 <emission>
		 <red>0.5</red>
		 <green>0.9</green>
		 <blue>0.5</blue>
		 <factor-prop>controls/switches/annunciators-norm</factor-prop>
	 </emission>
</animation>


<animation>
	<type>select</type>
	<object-name>LOC</object-name> 
        <condition>  
         <equals> 
          <property>instrumentation/nav/nav-loc</property>
          <value>1</value> 
         </equals>  
         <equals> 
          <property>instrumentation/nav[1]/nav-loc</property>
          <value>1</value> 
         </equals>
        </condition>
</animation>
 <animation>
	 <type>material</type>
	 <object-name>LOC</object-name>
	 <emission>
		 <red>0.5</red>
		 <green>0.9</green>
		 <blue>0.5</blue>
		 <factor-prop>controls/switches/annunciators-norm</factor-prop>
	 </emission>
</animation>

 <animation>
	 <type>material</type>
	 <object-name>rga</object-name>
	 <emission>
		 <red>0.5</red>
		 <green>0.9</green>
		 <blue>0.5</blue>
		 <factor-prop>controls/switches/annunciators-norm</factor-prop>
	 </emission>
</animation>


 <animation>
	 <type>material</type>
	 <object-name>switch_eject</object-name>
	 <emission>
		<red>0.9</red>
		 <green>0.5</green>
		 <blue>0.9</blue>
		 <factor-prop>controls/lighting/instruments-norm</factor-prop>
	</emission>
</animation>



<!-- Label Text -->

<text>
	<name>LabelText</name>
	<type type="string">literal</type>
		<text type="string">Caution</text>
	<draw-text type="bool">true</draw-text>
	<font>Helvetica.txf</font>
		<character-size type="double">0.012</character-size>
		<character-aspect-ratio type="double">1.0</character-aspect-ratio>
	<axis-alignment>yz-plane</axis-alignment>
	<max-height>0.2</max-height>
	<max-width>0.07</max-width>
	<font-resolution>
		<width type="int">32</width>
		<height type="int">32</height>
	</font-resolution>
	<alignment>center-center</alignment>
	<offsets>
		<x-m> 0.04</x-m>
		<y-m> 0.0 </y-m>
		<z-m> 0.0 </z-m>
	</offsets>
</text>

<animation>
	<type>material</type>
		<object-name>LabelText</object-name>
	<emission>
		<red>1.0</red>
		<green>0.8</green>
		<blue>0.0</blue>
	</emission>
</animation>

<animation>
	<type>select</type>
		<object-name>LabelText</object-name>
	<condition>
		<property>/sim/panel-hotspots</property>
	</condition>
</animation>


</PropertyList>
