<?xml version="1.0"?>
<!--
sweat by maggse
    -->
<PropertyList>

 <path>caution_ind.ac</path>
 
 <!-- eject switch -->
 
 <animation>
	 <type>rotate</type>
	 <object-name>switch_eject</object-name>
	 <property>controls/switches/prep-eject</property>
	 <interpolation>
		 <entry>
			 <ind>0.0</ind>
			 <dep>30.0</dep>
		 </entry>
		 <entry>
			 <ind>1.0</ind>
			 <dep>-30.0</dep>
		 </entry>
	 </interpolation>
	 <axis>
		 <x>0.0</x>
		 <y>1.0</y>
		 <z>0.0</z>
	 </axis>
	 <center>
		 <x-m>0.005</x-m>
		 <y-m>-0.424</y-m>
		 <z-m>0.084</z-m>
	 </center>
 </animation>
 
 <!-- clickable buttons block (osg only!) xxxxxxxxxxxxxxxxxxxxxxxxxxxxxx-->
 
 <animation>
	 <type>pick</type>
	 <object-name>switch_eject</object-name>
	 <action>
		 <button>0</button>
		 <repeatable>true</repeatable>
		 <interval-sec>0.5</interval-sec>
		 <binding>
			 <command>property-toggle</command>
			 <property>controls/switches/prep-eject</property>
		 </binding>
	 </action>
</animation>
 

<!-- caution ind -->

<animation>
	<type>material</type>
	<object-name>rep_mstrcaution</object-name>
	<emission>
		<red>0.7</red>
		<green>0.5</green>
		<blue>0.5</blue>
		<factor-prop>controls/switches/prep-eject</factor-prop>
	</emission>
</animation>


 <animation>
	 <type>select</type>
	 <object-name>rep_mstrcaution</object-name>
	 <condition>
		 <equals>
			 <property>controls/switches/prep-eject</property>
			 <value>1.0</value>
		 </equals>
	 </condition>
 </animation>
 
 <animation>
	 <type>select</type>
	 <object-name>rep_eject</object-name>
	 <condition>
		 <equals>
