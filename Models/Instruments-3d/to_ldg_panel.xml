<?xml version="1.0"?> 
<PropertyList> 
 <path>TO-LD.ac</path>  

**********Parking Brake**********

 <animation>
   <type>translate</type>
   <object-name>Parking-brake</object-name>
   <property>/controls/gear/brake-parking</property>v
   <factor>-0.002</factor>  
   <center> 
    <x-m>-0.026</x-m> 
    <y-m>-0.001</y-m> 
    <z-m>0.000</z-m> 
   </center> 
   <axis> 
    <x>0</x> 
    <y>0</y> 
    <z>1</z> 
   </axis> 
  </animation>

  <animation> 
   <type>pick</type> 
   <object-name>Parking-brake</object-name>
   <action> 
    <button>0</button>
      <binding>
        <command>property-toggle</command>
        <property>/controls/gear/brake-parking</property>                
        <repeatable>false</repeatable>
       </binding>
    </action> 
  </animation> 

**********Anti skid switch**********

  <animation>
   <type>rotate</type>
   <object-name>Anti-skid-switch</object-name>
   <property>/controls/gear/antiskid</property>v
   <factor>-45</factor> 
   <min>1</min>
   <max>0</max>   
   <center> 
    <x-m>-0.026</x-m> 
    <y-m>-0.047</y-m> 
    <z-m>0.000</z-m> 
   </center> 
   <axis> 
    <x>0</x> 
    <y>-1</y> 
    <z>0</z> 
   </axis> 
  </animation>

  <animation> 
   <type>pick</type> 
   <object-name>Anti-skid-switch</object-name>
   <action> 
    <button>0</button>
      <binding>
        <command>property-toggle</command>
        <property>/controls/gear/antiskid</property>                
        <repeatable>false</repeatable>
      </binding>
   </action> 
  </animation> 

**********NWS**********

  <animation>
   <type>rotate</type>
   <object-name>TO/LDG-switch</object-name>
   <property>gear/gear/caster-angle-deg</property>
   <factor>50</factor> 
   <min>0</min>
   <max>1</max>   
   <center> 
    <x-m>0.047</x-m> 
    <y-m>0.000</y-m> 
    <z-m>0.001</z-m> 
   </center> 
   <axis> 
    <x>0</x> 
    <y>1</y> 
    <z>0</z> 
   </axis> 
  </animation>

  <animation> 
   <type>pick</type> 
   <object-name>TO/LDG-switch</object-name>
   <action> 
    <button>0</button>
      <binding>
        <command>property-multiply</command>
        <property>gear/gear/caster-angle-deg</property>                
        <factor>2</factor> 
        <min>0</min> 
        <max>1</max>
      </binding>
   </action> 
 </animation>
</PropertyList>
