<PropertyList>

 <path>hud_alt.ac</path>


 

 <animation>
   <type>textranslate</type>
   <object-name>ind_alt_t</object-name>
   <property>instrumentation/altimeter/indicated-altitude-ft</property>
<interpolation>
     <entry>
             <ind>0</ind>
             <dep>0</dep>
     </entry>
     <entry>
	     <ind>49000</ind>
	     <dep>0.96</dep>
     </entry>
</interpolation>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
  <center>
	  <x-m>0.00</x-m>
	  <y-m>0.00</y-m>
	  <z-m>0.00</z-m>
   </center>
 </animation>
 
 <animation>
	 <type>textranslate</type>
	 <object-name>ind_alt_h</object-name>
	 <property>instrumentation/altimeter/indicated-altitude-ft</property>
	 <factor>0.001</factor>
		<offset>0</offset>
		 <axis>
			 <x>0</x>
			 <y>1</y>
			 <z>0</z>
		 </axis>
		 <center>
			 <x-m>0.00</x-m>
			 <y-m>0.00</y-m>
			 <z-m>0.00</z-m>
		 </center>
 </animation>
 
 <animation>
	 <type>translate</type>
	 <object-name>ind_alt_lmob</object-name>
	 <property>instrumentation/vertical-speed-indicator/indicated-speed-fpm</property>
	 <!--<property>autopilot/settings/vertical-speed-fpm</property>-->
	 <interpolation>
		 <entry>
			 <ind>-10000</ind>
			 <dep>-0.045</dep>
		 </entry>
		 <entry>
			 <ind>-1000</ind>
			 <dep>-0.030</dep>
		 </entry>
		 <entry>
			 <ind>-500</ind>
			 <dep>-0.015</dep>
		 </entry>
		 <entry>
			 <ind>0</ind>
			 <dep>0</dep>
		 </entry>
		 <entry>
			 <ind>500</ind>
			 <dep>0.015</dep>
		 </entry>
		 <entry>
			 <ind>1000</ind>
			 <dep>0.030</dep>
		 </entry>
		 <entry>
			 <ind>10000</ind>
			 <dep>0.045</dep>
		 </entry>
	 </interpolation>
	 <axis>
		 <x>0</x>
		 <y>0</y>
		 <z>1</z>
	 </axis>
	 <center>
		 <x-m>0.00</x-m>
		 <y-m>0.00</y-m>
		 <z-m>0.00</z-m>
	 </center>
 </animation>
 
 <animation>
	 <type>pick</type>
	 <object-name>switch_fcgms_mode.001</object-name>
	 <action>
		 <button>0</button>
		 <repeatable>true</repeatable>
		 <interval-sec>0.1</interval-sec>
		 <binding>
			 <command>property-adjust</command>
			 <property>autopilot/settings/target-altitude-ft</property>
			 <step>100</step>
			 <min>0.0</min>
			 <max>49000</max>
		 </binding>
	 </action>
	 <action>
		 <button>1</button>
		 <repeatable>true</repeatable>
		 <interval-sec>0.1</interval-sec>
		 <binding>
			 <command>property-adjust</command>
			 <property>autopilot/settings/target-altitude-ft</property>
			 <step>-100</step>
			 <min>0.0</min>
			 <max>49000</max>
		 </binding>
	 </action>
 </animation>
 
 <animation>
	 <type>textranslate</type>
	 <object-name>altft_10000</object-name>
	 <property>autopilot/settings/target-altitude-ft</property>
	 <factor>0.00001</factor>
	 <step>10000</step>
	 <axis>
		 <x>0</x>
		 <y>1</y>
		 <z>0</z>
	 </axis>
 </animation>
 
 <animation>
	 <type>textranslate</type>
	 <object-name>altft_1000</object-name>
	 <property>autopilot/settings/target-altitude-ft</property>
	 <factor>0.0001</factor>
	 <step>1000</step>
	 <axis>
		 <x>0</x>
		 <y>1</y>
		 <z>0</z>
	 </axis>
 </animation>
 
 <animation>
	 <type>textranslate</type>
	 <object-name>altft_100</object-name>
	 <property>autopilot/settings/target-altitude-ft</property>
	 <factor>0.001</factor>
	 <step>100</step>
	 <axis>
		 <x>0</x>
		 <y>1</y>
		 <z>0</z>
	 </axis>
 </animation>
 
 <animation>
	 <type>textranslate</type>
	 <object-name>altft_10</object-name>
	 <property>autopilot/settings/target-altitude-ft</property>
	 <factor>0.01</factor>
	 <step>10</step>
	 <axis>
		 <x>0</x>
		 <y>1</y>
		 <z>0</z>
	 </axis>
 </animation>
 
 <animation>
	 <type>textranslate</type>
	 <object-name>altft_1</object-name>
	 <property>autopilot/settings/target-altitude-ft</property>
	 <factor>0.1</factor>
	 <step>1</step>
	 <axis>
		 <x>0</x>
		 <y>1</y>
		 <z>0</z>
	 </axis>
 </animation>
 
 <animation>
	 <type>material</type>
	 <object-name>ind_alt_t</object-name>
	 <object-name>ind_alt_h</object-name>
	 <object-name>switch_fcgms_mode.001</object-name>
	 <object-name>altft_1</object-name>
	 <object-name>altft_10</object-name>
	 <object-name>altft_100</object-name>
	 <object-name>altft_1000</object-name>
	 <object-name>altft_10000</object-name>
	 <object-name>ind_alt_lmob.001</object-name>
	 <object-name>ind_alt_l</object-name>
	 <object-name>ind_alt_lmob</object-name>
	 <emission>
		 <red>0.6</red>
		 <green>0.4</green>
		 <blue>0.3</blue>
		 <factor-prop>controls/lighting/instruments-norm</factor-prop>
	 </emission>
</animation>


</PropertyList>