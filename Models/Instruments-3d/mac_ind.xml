<?xml version="1.0"?>
<!--
sweat by maggse
    -->
<PropertyList>

 <path>mac_ind.ac</path>
 

 <!-- cg mac -->

 <animation>
	 <type>translate</type>
	 <object-name>macind1</object-name>
	 <object-name>macind2</object-name>
	 <property>instrumentation/cg/cg_mac</property>
	 <interpolation>
		 <entry>
			 <ind>0.0</ind>
			 <dep>0.0</dep>
		 </entry>
		 <entry>
			 <ind>70.0</ind>
			 <dep>0.111</dep>
		 </entry>
	 </interpolation>
	 <axis>
		 <x>0.0</x>
		 <y>0.0</y>
		 <z>-1.0</z>
	 </axis>
 </animation>
 <animation>
	 <type>material</type>
	 <object-name>macind1</object-name>
	 <object-name>macind2</object-name>
	 <emission>
		 <red>0.8</red>
		 <green>0.4</green>
		 <blue>0.3</blue>
		 <factor-prop>controls/lighting/instruments-norm</factor-prop>
	 </emission>
</animation>

<animation>
	<type>translate</type>
	<object-name>macindset1</object-name>
	<object-name>macindset2</object-name>
	<property>instrumentation/cg/cg_mac_set</property>
	<interpolation>
		<entry>
			<ind>0.0</ind>
			<dep>0.0</dep>
		</entry>
		<entry>
			<ind>70.0</ind>
			<dep>0.111</dep>
		</entry>
	</interpolation>
	<axis>
		<x>0.0</x>
		<y>0.0</y>
		<z>-1.0</z>
	</axis>
</animation>
<animation>
	<type>material</type>
	<object-name>macind1</object-name>
	<object-name>macind2</object-name>
	<emission>
		<red>0.8</red>
		<green>0.4</green>
		<blue>0.3</blue>
		<factor-prop>controls/lighting/instruments-norm</factor-prop>
	</emission>
</animation>
 <!-- total gross weight -->
<animation>
	<type>textranslate</type>
	<object-name>macfuel1.001</object-name>
	<property>yasim/gross-weight-lbs</property>
	<factor>0.000001</factor>
	<step>100000</step>
	<axis>
		<x>0</x>
		<y>1</y>
		<z>0</z>
	</axis>
 </animation>
 
