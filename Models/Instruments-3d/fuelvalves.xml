<?xml version="1.0"?>
<!--
sweat by maggse
    -->
<PropertyList>

 <path>fuelvalves.ac</path>
 

 <!-- fuel valves tank3-->

 <animation>
	 <type>rotate</type>
	 <object-name>switch_1</object-name>
	 <property>consumables/fuel/tank[2]/selected</property>
	 <interpolation>
		 <entry>
			 <ind>0.0</ind>
			 <dep>30.0</dep>
		 </entry>
		 <entry>
			 <ind>1.0</ind>
			 <dep>-30.0</dep>
		 </entry>
	 </interpolation>
	 <axis>
		 <x>0.0</x>
		 <y>1.0</y>
		 <z>0.0</z>
	 </axis>
	 <center>
		 <x-m>0.005</x-m>
		 <y-m>-0.028</y-m>
		 <z-m>-0.051</z-m>
   </center>
 </animation>
 
 <!-- clickable buttons block (osg only!) xxxxxxxxxxxxxxxxxxxxxxxxxxxxxx-->
 
 <animation>
	 <type>pick</type>
	 <object-name>switch_1</object-name>
	 <action>
		 <button>0</button>
		 <repeatable>true</repeatable>
		 <interval-sec>0.1</interval-sec>
		 <binding>
			 <command>property-toggle</command>
			 <property>consumables/fuel/tank[2]/selected</property>
		 </binding>
	 </action>
</animation>

