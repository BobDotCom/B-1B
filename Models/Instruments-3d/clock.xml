<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>fg-clock.ac</path>    ### NOT to be used in other variants of the BONE ###

  <animation>
    <object-name>AdjKnob.h</object-name>
    <object-name>AdjKnob.m</object-name>
    <object-name>fond</object-name>
    <object-name>tenths</object-name>
    <object-name>face</object-name>
    <object-name>hour</object-name>
    <object-name>minute</object-name>
    <object-name>second</object-name>

  </animation>

  <animation>
    <type>material</type>
    <object-name>tenths</object-name>
    <object-name>face</object-name>
    <object-name>hour</object-name>
    <object-name>minute</object-name>
    <object-name>second</object-name>
	 <emission>
		 <red>0.1</red>
		 <green>0.9</green>
		 <blue>0.5</blue>
		 <factor-prop>controls/lighting/instruments-norm</factor-prop>
	</emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>hour</object-name>
    <property>/instrumentation/clock/indicated-sec</property>
    <factor>0.00833333333333333</factor>
      <center>
   <x-m> 0.005613</x-m>
   <y-m>-0.009924</y-m>
   <z-m> 0.010004</z-m>
  </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>minute</object-name>
    <property>/instrumentation/clock/indicated-sec</property>
    <factor>0.1</factor>
      <center>
   <x-m> 0.005713</x-m>
   <y-m>-0.009866</y-m>
   <z-m> 0.009876</z-m>
  </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>second</object-name>
    <property>/instrumentation/clock/indicated-sec</property>
    <factor>6</factor>
      <center>
   <x-m> 0.00569</x-m>
   <y-m>-0.01006</y-m>
   <z-m> 0.009957</z-m>
  </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>tenths</object-name>
    <property>/instrumentation/clock/indicated-sec</property>
    <factor>1</factor>
      <center>
   <x-m> 0.005699</x-m>
   <y-m>-0.010095</y-m>
   <z-m> 0.000319</z-m>
  </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
