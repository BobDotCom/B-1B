 <!-- Refuel-->

 <animation>
	 <type>translate</type>
	 <object-name>refuel-lid</object-name>
	 <property>controls/switches/refuel-lid</property>
	 <interpolation>
		 <entry>
			 <ind>0.0</ind>
			 <dep>0.0</dep>
		 </entry>
		 <entry>
			 <ind>1.0</ind>
			 <dep>0.01</dep>
		 </entry>
	 </interpolation>
	 <axis>
		 <x>1.0</x>
		 <y>0.0</y>
		 <z>0.0</z>
	 </axis>
	 <center>
		 <x-m>0.04808</x-m>
		 <y-m>-0.04141</y-m>
		 <z-m>0.22288</z-m>
   </center>
 </animation>

 <animation>
	 <type>pick</type>
	 <object-name>refuel-lid</object-name>
	 <action>
		 <button>0</button>
		 <repeatable>true</repeatable>
		 <interval-sec>1.0</interval-sec>
		 <binding>
			 <command>property-toggle</command>
			 <property>controls/switches/refuel-lid</property>
			 <condition>
				 <property>controls/switches/refuel-slipway</property>
				 <value>1</value>
			 </condition>
		 </binding>
		 <binding>
			 <command>nasal</command>
			 <script>b1b.refuel_door()</script>
			 <condition>
				 <property>controls/switches/refuel-slipway</property>
				 <value>1</value>
			 </condition>
		 </binding>
		 <binding>
			 <command>property-toggle</command>
			 <property>systems/refuel/serviceable</property>
			 <condition>
				 <property>controls/switches/refuel-slipway</property>
				 <value>1</value>
			 </condition>
		 </binding>
	 </action>
</animation>

