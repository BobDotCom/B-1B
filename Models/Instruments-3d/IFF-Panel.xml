    <object-name>Switch-5</object-name>
    <object-name>Switch-6</object-name>
    <object-name>Switch-7</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>IFF-PANEL</object-name>
    <object-name>knob_oh.4</object-name>
    <object-name>knob_oh.020</object-name>
    <object-name>Left-hand-large-switch</object-name>
    <object-name>Right-hand-large-switch</object-name>
    <object-name>Small-knob-1</object-name>
    <object-name>Small-knob-2</object-name>
    <object-name>switch_oh.000</object-name>
    <object-name>switch_oh.28</object-name>
    <object-name>switch_oh.035</object-name>
    <object-name>switch_oh.41</object-name>
    <object-name>switch_oh.052</object-name>
    <object-name>switch_oh.057</object-name>
    <object-name>switch_oh.058</object-name>
    <object-name>switch_oh.059</object-name>
    <object-name>button.001</object-name>
    <object-name>button.002</object-name>
    <object-name>knob_oh.009</object-name>
    <object-name>knob_oh.016</object-name>
    <object-name>knob_oh.017</object-name>
    <object-name>knob_oh.018</object-name>
    <object-name>knob_oh.019</object-name>
	 <emission>
		 <red>0.9</red>
		 <green>0.5</green>
		 <blue>0.9</blue>
		 <factor-prop>controls/lighting/centre_console</factor-prop>
	</emission>
  </animation>

<animation>
	<type>knob</type>
	 <object-name>M.1_switch</object-name>
	<property>instrumentation/iff/M.1_switch</property>
	<factor>20</factor>
	<center>
		<x-m>0.00627</x-m>
		<y-m>-0.026</y-m>
		<z-m>0.006</z-m>
	</center>
	 <axis>
		 <x>0.0</x>
		 <y>-1.0</y>
		 <z>0.0</z>
	 </axis>


	 <action>
		 <binding>
			 <command>property-adjust</command>
			 <property>instrumentation/iff/M.1_switch</property>
			 <min>-1</min>
			 <max>1</max>
		 </binding>
	 </action>
	<release>
		<binding>
			<command>property-adjust</command>
			<property>instrumentation/iff/M.1_switch</property>
			<step>0</step>
			<min>-1</min>
			<max>0</max>
		</binding>
	</release>
</animation>

<animation>
	<type>knob</type>
	 <object-name>M.3A_switch</object-name>
	<property>instrumentation/iff/M.3A_switch</property>
	<factor>20</factor>
	<center>
		<x-m>0.00605</x-m>
		<y-m>-0.0137</y-m>
		<z-m>0.006</z-m>
	</center>
	 <axis>
		 <x>0.0</x>
		 <y>-1.0</y>
		 <z>0.0</z>
	 </axis>


	 <action>
		 <binding>
			 <command>property-adjust</command>
			 <property>instrumentation/iff/M.3A_switch</property>
			 <min>-1</min>
			 <max>1</max>
		 </binding>
	 </action>
	<release>
		<binding>
			<command>property-adjust</command>
			<property>instrumentation/iff/M.3A_switch</property>
			<step>0</step>
			<min>-1</min>
			<max>0</max>
		</binding>
	</release>
</animation>

  <animation>
        <type>knob</type>
        <object-name>Master</object-name>
        <property>instrumentation/iff/master-switch</property>
        <factor>45</factor>
        <center>
            <x-m>-0.03488</x-m>
            <y-m>0.03866</y-m>
            <z-m>0.0088</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>-1</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/iff/master-switch</property>
                <min>0</min>
                <max>4</max>
                <wrap>false</wrap>
           </binding>
        </action>
    </animation>

