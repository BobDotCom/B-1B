<?xml version="1.0"?>
<PropertyList>

 <path>IFF-Panel.ac</path>

  <animation>
    <object-name>IFF-PANEL</object-name>
    <object-name>Knob-1</object-name>
    <object-name>Knob-2</object-name>
    <object-name>Left-hand-large-switch</object-name>
    <object-name>Right-hand-large-switch</object-name>
    <object-name>Small-knob1</object-name>
    <object-name>Small-knob2</object-name>
    <object-name>Switch-1</object-name>
    <object-name>Switch-2</object-name>
    <object-name>Switch-3</object-name>
    <object-name>Switch-4</object-name>
    <object-name>Switch-5</object-name>
    <object-name>Switch-6</object-name>
    <object-name>Switch-7</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>IFF-PANEL</object-name>
    <object-name>knob_oh.4</object-name>
    <object-name>knob_oh.020</object-name>
    <object-name>Left-hand-large-switch</object-name>
    <object-name>Right-hand-large-switch</object-name>
    <object-name>Small-knob-1</object-name>
    <object-name>Small-knob-2</object-name>
    <object-name>switch_oh.000</object-name>
    <object-name>switch_oh.28</object-name>
    <object-name>switch_oh.035</object-name>
    <object-name>switch_oh.41</object-name>
    <object-name>switch_oh.052</object-name>
    <object-name>switch_oh.057</object-name>
    <object-name>switch_oh.058</object-name>
    <object-name>switch_oh.059</object-name>
    <object-name>button.001</object-name>
    <object-name>button.002</object-name>
    <object-name>knob_oh.009</object-name>
    <object-name>knob_oh.016</object-name>
    <object-name>knob_oh.017</object-name>
    <object-name>knob_oh.018</object-name>
    <object-name>knob_oh.019</object-name>
	 <emission>
		 <red>0.9</red>
		 <green>0.5</green>
		 <blue>0.9</blue>
		 <factor-prop>controls/lighting/centre_console</factor-prop>
	</emission>
  </animation>

  <animation>
        <type>knob</type>
        <object-name>Mode_1_dial</object-name>
        <property>controls/switches/IFF_dial_1</property>
        <factor>45</factor>
        <center>
            <x-m>0.0239</x-m>
            <y-m>-0.0262</y-m>
            <z-m>0.0065</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/switches/IFF_dial_1</property>
                <min>0</min>
                <max>99</max>
                <wrap>false</wrap>
           </binding>
        </action>
    </animation>

  <animation>
        <type>knob</type>
        <object-name>Mode_3a_dial_1</object-name>
        <property>controls/switches/IFF_dial_2</property>
        <factor>45</factor>
        <center>
            <x-m>0.0239</x-m>
            <y-m>0.0039</y-m>
            <z-m>0.0065</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/switches/IFF_dial_2</property>
                <min>0</min>
                <max>99</max>
                <wrap>false</wrap>
           </binding>
        </action>
    </animation>

  <animation>
        <type>knob</type>
        <object-name>Mode_3a_dial_2</object-name>
        <property>controls/switches/IFF_dial_3</property>
        <factor>45</factor>
        <center>
            <x-m>0.0239</x-m>
            <y-m>0.0338</y-m>
            <z-m>0.0065</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/switches/IFF_dial_3</property>
                <min>0</min>
                <max>99</max>
                <wrap>false</wrap>
           </binding>
        </action>
    </animation>

