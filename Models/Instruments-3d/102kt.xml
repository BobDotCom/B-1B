<?xml version="1.0"?>

<PropertyList>
   <path>102kt.ac</path>
   <animation>
    <type>material</type>
       <object-name>Cube</object-name>
	   <emission>
		 <red>0.9</red>
		 <green>0.5</green>
		 <blue>0.9</blue>
		 <factor-prop>controls/lighting/instruments-norm</factor-prop>
	  </emission>
    </animation> 

  <animation>
   <type>textranslate</type>
   <object-name>1000s</object-name>
   <condition>
       <equals>
            <property>B1b/airspeed-indicator</property>
            <value>1</value>
       </equals>
   </condition>
   <property>velocities/airspeed-kt</property>
   <factor>0.0001</factor> 
   <step>1000</step> 
   <axis> 
    <x>0</x> 
    <y>-1</y> 
    <z>0</z> 
   </axis> 
  </animation> 

  <animation>
   <type>textranslate</type>
   <object-name>1000s</object-name>
   <condition>
       <equals>
            <property>B1b/airspeed-indicator</property>
            <value>2</value>
       </equals>
   </condition>
   <property>velocities/groundspeed-kt</property>
   <factor>0.0001</factor>  
   <step>1000</step> 
   <axis> 
    <x>0</x> 
    <y>-1</y> 
    <z>0</z> 
   </axis> 
  </animation> 

  <animation>
   <type>textranslate</type>
   <object-name>100s</object-name>
   <condition>
       <equals>
            <property>B1b/airspeed-indicator</property>
            <value>1</value>
       </equals>
   </condition>
   <property>velocities/airspeed-kt</property>
   <factor>0.001</factor> 
   <step>100</step> 
   <axis> 
    <x>0</x> 
    <y>-1</y> 
    <z>0</z> 
   </axis> 
  </animation> 

  <animation>
   <type>textranslate</type>
   <object-name>100s</object-name>
   <condition>
       <equals>
            <property>B1b/airspeed-indicator</property>
            <value>2</value>
       </equals>
   </condition>
   <property>velocities/groundspeed-kt</property>
   <factor>0.001</factor>  
   <step>100</step> 
   <axis> 
    <x>0</x> 
    <y>-1</y> 
    <z>0</z> 
   </axis> 
  </animation> 

  <animation>
   <type>textranslate</type>
   <object-name>10s</object-name>
   <condition>
       <equals>
            <property>B1b/airspeed-indicator</property>
            <value>1</value>
       </equals>
   </condition>
   <property>velocities/airspeed-kt</property>
   <factor>0.01</factor> 
   <step>10</step> 
   <axis> 
    <x>0</x> 
    <y>-1</y> 
    <z>0</z> 
   </axis> 
  </animation> 

  <animation>
   <type>textranslate</type>
   <object-name>10s</object-name>
   <condition>
       <equals>
            <property>B1b/airspeed-indicator</property>
            <value>2</value>
       </equals>
   </condition>
   <property>velocities/groundspeed-kt</property>
   <factor>0.01</factor> 
   <step>10</step>   
   <axis> 
    <x>0</x> 
    <y>-1</y> 
    <z>0</z> 
   </axis> 
  </animation> 

  <animation>
   <type>textranslate</type>
   <object-name>10ths</object-name>
   <condition>
       <equals>
            <property>B1b/airspeed-indicator</property>
            <value>1</value>
       </equals>
   </condition>
   <property>velocities/airspeed-kt</property>
   <condition>
       <equals>
            <property>B1b/airspeed-indicator</property>
            <value>2</value>
       </equals>
   </condition>
   <property>velocities/groundspeed-kt</property>
   <factor>0.1</factor> 
   <step>1</step> 
   <axis> 
    <x>0</x> 
    <y>-1</y> 
    <z>0</z> 
   </axis> 
  </animation> 
  

  <animation>
        <type>knob</type>
        <object-name>needle</object-name>
        <property>B1b/airspeed-indicator</property>
        <factor>45</factor>
        <center>
            <x-m>0.0065</x-m>
            <y-m>0.0086</y-m>
            <z-m>-0.0115</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>        
        <action>
            <binding>
                <command>property-cycle</command>
                <property>B1b/airspeed-indicator</property>
                <value>0</value>
                <value>1</value>
                <value>2</value>
                <wrap>false</wrap>
           </binding>
        </action>
    </animation>

</PropertyList>

