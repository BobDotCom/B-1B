<?xml version="1.0"?>
<!--
sweat by maggse
    -->
<PropertyList>

 <path>throttle.ac</path>

<!-- flap switch -->

<animation>
	<type>rotate</type>
	<object-name>switch_flaps.002</object-name>
	<property>controls/flight/flaps</property>
	<interpolation>
		<entry>
			<ind>0.0</ind>
			<dep>0.0</dep>
		</entry>
		<entry>
			<ind>0.5</ind>
			<dep>38</dep>
		</entry>
		<entry>
			<ind>1.0</ind>
			<dep>65.0</dep>
		</entry>
	</interpolation>
	<axis>
		<x>0.0</x>
		<y>1.0</y>
		<z>0.0</z>
	</axis>
	<center>
		<x-m>0.002</x-m>
		<y-m>-0.205</y-m>
		<z-m>0.250</z-m>
	</center>
</animation>
<animation>
	<type>material</type>
	<object-name>switch_flaps.002</object-name>
	<emission>
		<red>0.6</red>
		<green>0.4</green>
		<blue>0.3</blue>
		<factor-prop>controls/lighting/instruments-norm</factor-prop>
	</emission>
</animation>

<!-- clickable buttons block (osg only!) xxxxxxxxxxxxxxxxxxxxxxxxxxxxxx-->

<animation>
	<type>pick</type>
	<object-name>switch_flaps.002</object-name>
	<action>
		<button>0</button>
		<repeatable>true</repeatable>
		<interval-sec>0.2</interval-sec>
		<binding>
			<command>property-adjust</command>
			<property>controls/flight/flaps</property>
			<step>0.5</step>
			<min>0.0</min>
			<max>1.0</max>
		</binding>
		<binding>
			<command>nasal</command>
			<script>controls.flapsDown(1)</script>
		</binding>
		<mod-up>
			<binding>
				<command>nasal</command>
				<script>controls.flapsDown(0)</script>
			</binding>
                </mod-up>
	</action>
	<action>
		<button>1</button>
		<repeatable>true</repeatable>
		<interval-sec>0.2</interval-sec>
		<binding>
			<command>property-adjust</command>
			<property>controls/flight/flaps</property>
			<step>-0.5</step>
			<min>0.0</min>
			<max>1.0</max>
		</binding>
		<binding>
			<command>nasal</command>
			<script>controls.flapsDown(-1)</script>
		</binding>
		<mod-up>
			<binding>
				<command>nasal</command>
				<script>controls.flapsDown(0)</script>
			</binding>
                </mod-up>
	</action>
</animation>

