  <animation>
        <type>knob</type>
        <object-name>TNK-SEL</object-name>
        <property>B-1B/fuel-tank-switch</property>
        <factor>45</factor>
        <center>
            <x-m>-18.47</x-m>
            <y-m> 0.358</y-m>
            <z-m>-0.2029</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y> 0</y>
            <z> 0</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>B-1B/fuel-tank-switch</property>
                <min>0</min>
                <max>5</max>
                <wrap>false</wrap>
           </binding>
        </action>
    </animation>

