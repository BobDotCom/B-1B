   <equals>
    <property>B-1B/fuel-tank-switch</property>
    <value>5</value>
   </equals>
  </condition>
  <property>consumables/fuel/tank[6]/level-lbs</property>
  <factor>0.0036</factor>
  <center>
   <x-m>-18.48718</x-m>
