  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>

  <animation>
        <type>knob</type>
        <object-name>blst_tk_isln</object-name>
        <property>B-1B/fuel-tank-switch</property>
        <factor>45</factor>
	<center>
		<x-m>-18.46500</x-m>
		<y-m> 0.43406</y-m>
		<z-m>-0.14496</z-m>
	</center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>B-1B/fuel-tank-switch</property>
                <min>0</min>
                <max>5</max>
                <wrap>false</wrap>
           </binding>
        </action>
    </animation>

  <animation>
        <type>knob</type>
        <object-name>TNK-SEL</object-name>
        <property>controls/fuel/tank</property>
        		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>1.0</dep>
			</entry>
			<entry>
				<ind>6.0</ind>
				<dep>-36.0</dep>
			</entry>
		</interpolation>
        <center>
            <x-m>-18.46894</x-m>
            <y-m> 0.035632</y-m>
            <z-m>-0.20877</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <action>
           <binding>
                <command>property-adjust</command>
                <property>controls/fuel/tank/to-tank</property>
           </binding>
        </action>
    </animation>

 <animation>
 <type>pick</type>
 <object-name>TNK-SEL</object-name>
 <action>
  <button>0</button>
   <binding>
    <command>property-toggle</command>
     <property>controls/fuel/tank</property>
                <step>-1</step>
				<min>0.0</min>
				<max>6.0</max>
   </binding>
  </action>
 </animation>

