<PropertyList>

<path>alter_throt.ac</path>

<!-- afterburner switches -->
<animation>
	<type>rotate</type>
	<object-name>eng_0</object-name>
	<property>controls/switches/engines/afterburner[0]</property>
	<interpolation>
		<entry>
			<ind>0.0</ind>
			<dep>30.0</dep>
		</entry>
		<entry>
			<ind>1.0</ind>
			<dep>-30.0</dep>
		</entry>
	</interpolation>
	<axis>
		<x>0.0</x>
		<y>1.0</y>
		<z>0.0</z>
	</axis>
	<center>
		<x-m>0.01273</x-m>
		<y-m>0.2201</y-m>
		<z-m>0.06411    </z-m>
	</center>
</animation>

<animation>
	<type>pick</type>
	<object-name>eng_0</object-name>
	<action>
		<button>0</button>
		<repeatable>true</repeatable>
		<interval-sec>0.3</interval-sec>
		<binding>
			<command>property-toggle</command>
			<property>controls/switches/engines/afterburner[0]</property>
		</binding>
	</action>
</animation>

