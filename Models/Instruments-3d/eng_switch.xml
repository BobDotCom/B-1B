<?xml version="1.0"?>
<!--
sweat by maggse
    -->
<PropertyList>

 <path>eng_switch.ac</path>

 <animation>
	 <type>material</type>
	 <object-name>switch_eng0</object-name>
	 <object-name>switch_eng1</object-name>
	 <object-name>switch_eng2</object-name>
	 <object-name>switch_eng3</object-name>
	 <object-name>fuel_cov.005</object-name>
	 <emission>
		 <red>0.9</red>
		 <green>0.5</green>
		 <blue>0.9</blue>
		 <factor-prop>controls/lighting/instruments-norm</factor-prop>
	</emission>
</animation>

 <animation>
	 <type>knob</type>
	 <object-name>switch_eng0</object-name>
	 <property>controls/engines/engine[0]/cutoff-switch</property>
	 <factor>-30</factor>
	 <center>
		 <x-m>0.00136</x-m>
		 <y-m>-0.44896</y-m>
		 <z-m>0.15779</z-m>
	 </center>
	 <axis>
		 <x>0.0</x>
		 <y>1.0</y>
		 <z>0.0</z>
	 </axis>


	 <action>
		 <binding>
			 <command>property-adjust</command>
			 <property>controls/engines/engine[0]/cutoff-switch</property>
			 <min>-1</min>
			 <max>1</max>
		 </binding>
	 </action>
</animation>

	<animation>
		<type>knob</type>
		<object-name>switch_eng1</object-name>
		<property>controls/engines/engine[1]/cutoff-switch</property>
		<factor>-30</factor>
		<center>
			<x-m>0.00136</x-m>
			<y-m>-0.42175</y-m>
			<z-m>0.15780</z-m>
		</center>
		<axis>
			<x>0.0</x>
			<y>1.0</y>
			<z>0.0</z>
		</axis>


		<action>
			<binding>
				<command>property-adjust</command>
				<property>controls/engines/engine[1]/cutoff-switch</property>
				<min>-1</min>
				<max>1</max>
			</binding>
		</action>
</animation>

	<animation>
		<type>knob</type>
		<object-name>switch_eng2</object-name>
		<property>controls/engines/engine[2]/cutoff-switch</property>
		<factor>-30</factor>
		<center>
			<x-m>0.00136</x-m>
			<y-m>-0.39317</y-m>
			<z-m>0.15776</z-m>
		</center>
		<axis>
			<x>0.0</x>
			<y>1.0</y>
			<z>0.0</z>
		</axis>


		<action>
			<binding>
				<command>property-adjust</command>
				<property>controls/engines/engine[2]/cutoff-switch</property>
				<min>-1</min>
