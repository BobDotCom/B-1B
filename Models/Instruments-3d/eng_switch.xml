<PropertyList>

 <path>eng_switch.ac</path>
 

 <!-- engines on/off -->

 <animation>
	 <type>rotate</type>
	 <object-name>switch_eng0</object-name>
	 <property>controls/engines/engine[0]/cutoff</property>
	 <interpolation>
		 <entry>
			 <ind>0.0</ind>
			 <dep>-30.0</dep>
		 </entry>
		 <entry>
			 <ind>1.0</ind>
			 <dep>30.0</dep>
		 </entry>
	 </interpolation>
	 <axis>
		 <x>0.0</x>
		 <y>1.0</y>
		 <z>0.0</z>
	 </axis>
	 <center>
		 <x-m>0.005</x-m>
		 <y-m>-0.449</y-m>
		 <z-m>0.148</z-m>
   </center>
 </animation>
 <animation>
	 <type>material</type>
	 <object-name>switch_eng0</object-name>
	 <emission>
		 <red>0.6</red>
		 <green>0.4</green>
		 <blue>0.3</blue>
		 <factor-prop>controls/lighting/instruments-norm</factor-prop>
	 </emission>
</animation>
 
 <!-- clickable buttons block (osg only!) xxxxxxxxxxxxxxxxxxxxxxxxxxxxxx-->
 
 <animation>
	 <type>pick</type>
	 <object-name>switch_eng0</object-name>
	 <action>
		 <button>0</button>
		 <repeatable>true</repeatable>
		 <interval-sec>0.5</interval-sec>
		 <binding>
			 <command>property-toggle</command>
			 <property>controls/engines/engine[0]/cutoff</property>
			 <condition>
		 <or>
	          <and>
		   <equals>
			 <property>controls/APU/APUL_running</property>
			 <value>1</value>
		   </equals>
		   <equals>
			 <property>controls/APU/APUL_couple</property>
			 <value>1</value>
		   </equals>
	          </and>
		  <and>
		   <equals>
			 <property>controls/APU/APUL_running</property>
			 <value>1</value>
		   </equals>
		   <equals>
			 <property>controls/APU/APUL_bleed</property>
			 <value>1</value>
		   </equals>
	          </and>
		  <and>
		   <equals>
			 <property>controls/APU/APUR_running</property>
			 <value>1</value>
		   </equals>
		   <equals>
			 <property>controls/APU/APU_crossbleed</property>
			 <value>1</value>
		   </equals>
	          </and>
		  <and>
		  <equals>
			 <property>controls/engines/engine[0]/cutoff</property>
			 <value>0</value>
		   </equals>
	         </and>
	 	 </or>
	         </condition>
		 </binding>
		 <binding>
			 <command>nasal</command>
			 <script>b1b.eng_state(0)</script>
		 </binding>
	 </action>
</animation>

