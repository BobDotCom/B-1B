<PropertyList>

 <path>radar_alt.ac</path>

 <animation>
   <type>rotate</type>
   <object-name>needle</object-name>
   <property>position/altitude-agl-ft</property>
<interpolation>
     <entry>
       <ind>0</ind>
       <dep>-5</dep>
     </entry>
     <entry>
       <ind>500</ind>
       <dep>-170</dep>
     </entry>
     <entry>
       <ind>1000</ind>
       <dep>-205</dep>
     </entry>
     <entry>
	     <ind>35000</ind>
	     <dep>-324</dep>
     </entry>
</interpolation>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
	  <x-m>0.041</x-m>
	  <y-m>-0.00</y-m>
	  <z-m>-0.00</z-m>
   </center>
 </animation>

  <animation>
    <type>rotate</type>
    <object-name>bug</object-name>
    <property>sim/model/jrb-usn/radar-altitude-decision-height</property>
    <interpolation>
      <entry><ind>    0 </ind><dep>   0 </dep></entry>
      <entry><ind>  200 </ind><dep> 212 </dep></entry>
      <entry><ind> 1000 </ind><dep> 272 </dep></entry>
      <entry><ind> 5000 </ind><dep> 320 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
 <animation>
	 <type>material</type>
	 <object-name>needle</object-name>
	 <object-name>Face</object-name>
     <object-name>bug</object-name>
	 <emission>
		 <red>0.3</red>
		 <green>0.0</green>
		 <blue>0.0</blue>
		 <factor-prop>controls/lighting/instruments-norm</factor-prop>
	</emission>
</animation>


</PropertyList>
