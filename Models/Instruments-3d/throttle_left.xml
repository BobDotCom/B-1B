<PropertyList>

<path>throttle_left.ac</path>

<!-- throttle switches -->

<animation>
	<type>rotate</type>
	<object-name>throttle_lever_1_0</object-name>
    <object-name>throttle_lever_1_1</object-name>
	<property>controls/engines/engine[0]/throttle-lever</property>
	<interpolation>
		<entry>
			<ind>0.0</ind>
			<dep>30.0</dep>
		</entry>
		<entry>
			<ind>0.5</ind>
			<dep>0</dep>
		</entry>
		<entry>
			<ind>1.0</ind>
			<dep>-30.0</dep>
		</entry>
	</interpolation>
	<axis>
		<x>0.0</x>
		<y>1.0</y>
		<z>0.0</z>
	</axis>
	<center>
		<x-m>0.03</x-m>
		<y-m>-0.02</y-m>
		<z-m>0.23</z-m>
	</center>
</animation>
<animation>
	<type>material</type>
	<object-name>throttle_lever_1</object-name>
	 <emission>
		 <red>0.9</red>
		 <green>0.5</green>
		 <blue>0.9</blue>
		 <factor-prop>controls/lighting/instruments-norm</factor-prop>
	</emission>
</animation>

<animation>
	<type>pick</type>
	<object-name>throttle_lever_1</object-name>
	<action>
		<button>0</button>
		<repeatable>true</repeatable>
		<interval-sec>0.2</interval-sec>
		<binding>
			<command>property-adjust</command>
			<property>controls/engines/engine[0]/throttle-lever</property>
			<step>0.1</step>
			<min>0.0</min>
			<max>1.0</max>
		</binding>
	</action>
	<action>
		<button>1</button>
		<repeatable>true</repeatable>
		<interval-sec>0.2</interval-sec>
		<binding>
			<command>property-adjust</command>
			<property>controls/engines/engine[0]/throttle-lever</property>
			<step>-0.1</step>
			<min>0.0</min>
			<max>1.0</max>
		</binding>
	</action>
</animation>

