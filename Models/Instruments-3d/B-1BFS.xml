<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>B-1BFS.ac</path>

  <animation>
    <object-name>Boot</object-name>
    <object-name>Cargo</object-name>
    <object-name>NWS_Base</object-name>
    <object-name>NWS_Switch</object-name>
    <object-name>RHFS</object-name>
    <object-name>Trim</object-name>
    <object-name>Wings</object-name>
    <object-name>screws</object-name> 
    <object-name>Stick_leg</object-name>
    <object-name>Stick</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>NWS_Switch</object-name>
    <object-name>Trim</object-name>
    <object-name>Wings</object-name>
	<emission>
		<red>0.9</red>
		<green>0.5</green>
		<blue>0.9</blue>
		<factor-prop>controls/lighting/instruments-norm</factor-prop>
	</emission>
  </animation>

 <animation>
  <type>rotate</type>
    <object-name>NWS_Base</object-name>
    <object-name>Cargo</object-name>
    <object-name>NWS_Switch</object-name>
    <object-name>RHFS</object-name>
    <object-name>Trim</object-name>
    <object-name>Wings</object-name>
    <object-name>Boot001</object-name>
    <object-name>Stick-leg</object-name>
    <object-name>Stick</object-name>
  <property>controls/flight/aileron</property>         <!--property alias="/params/aileron"/-->
  <factor>-10</factor>
  <center>
   <x-m>-18.47</x-m>
   <y-m>-0.5184</y-m>
   <z-m> 0.1275</z-m>
  </center>
  <axis>
   <x>1.0</x>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
    <object-name>NWS_Base</object-name>
    <object-name>Cargo</object-name>
    <object-name>NWS_Switch</object-name>
    <object-name>RHFS</object-name>
    <object-name>Trim</object-name>
    <object-name>Wings</object-name>
    <object-name>Boot001</object-name>
    <object-name>Stick-leg</object-name>
    <object-name>Stick</object-name>
  <property>controls/flight/elevator</property>          <!--property alias="/params/elevator"/-->
  <factor>-10</factor>
  <!-- offset-deg>-10.0</offset-deg --> 
  <center>
   <x-m>-18.47</x-m>
   <y-m>-0.5184</y-m>
   <z-m> 0.1275</z-m>
  </center>
  <axis>
   <y>1.0</y>
  </axis>
 </animation>

</PropertyList>
