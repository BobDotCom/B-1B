<?xml version="1.0"?>
 
<PropertyList>

<path>TO-LDG.ac</path>

  <animation>
    <object-name>Anti-skid-switch</object-name>
    <object-name>Data-case-lt</object-name>
    <object-name>Emerg</object-name>
    <object-name>Nut.1</object-name>
    <object-name>Nut.2</object-name>
    <object-name>Nut.3</object-name>
    <object-name>Parking-brake</object-name>
    <object-name>PowerNut</object-name>
    <object-name>TO-LD-Switch</object-name>
    <object-name>TO-LDG-Panel</object-name>
    <object-name>light-land-toggle</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>Anti-skid-switch</object-name>
    <object-name>Data-case-lt</object-name>
    <object-name>Emerg</object-name>
    <object-name>Nut-1</object-name>
    <object-name>Nut-2</object-name>
    <object-name>Nut-3</object-name>
    <object-name>Parking-brake</object-name>
    <object-name>PowerNut</object-name>
    <object-name>TO-LD-Switch</object-name>
    <object-name>TO-LDG-Panel</object-name>
    <object-name>light-land-toggle</object-name>
	 <diffuse>
		<red>0.9</red>
	   <green>0.1</green>
	   <blue>0.2</blue>
	 </diffuse>
         <ambient>
	   <red>0.9</red>
	   <green>0.1</green>
	   <blue>0.2</blue>
	 </ambient>
	 <emission>
		 <red>0.9</red>
		 <green>0.1</green>
		 <blue>0.2</blue>
		 <factor-prop>controls/lighting/instruments-norm</factor-prop>
	</emission>
  </animation>

 <animation>
   <type>translate</type>
   <object-name>Parking-brake</object-name>
   <property>/controls/gear/brake-parking</property>v
   <factor>-0.002</factor>  
   <center> 
    <x-m>-0.026</x-m> 
    <y-m>-0.001</y-m> 
    <z-m>0.000</z-m> 
   </center> 
   <axis> 
    <x>0</x> 
    <y>0</y> 
    <z>1</z> 
   </axis> 
  </animation>

  <animation> 
   <type>pick</type> 
   <object-name>Parking-brake</object-name>
   <action> 
    <button>0</button>
      <binding>
        <command>property-toggle</command>
        <property>/controls/gear/brake-parking</property>                
        <repeatable>false</repeatable>
       </binding>
    </action> 
  </animation> 

  <animation>
   <type>rotate</type>
   <object-name>Anti-skid-switch</object-name>
   <property>/controls/gear/antiskid</property>v
   <factor>-35</factor> 
   <min>1</min>
   <max>0</max>   
   <center> 
    <x-m>-0.02685</x-m> 
    <y-m>-0.04706</y-m> 
    <z-m> 0.00532</z-m> 
   </center> 
   <axis> 
    <x>0</x> 
    <y>-1</y> 
    <z>0</z> 
   </axis> 
  </animation>

  <animation> 
   <type>pick</type> 
   <object-name>Anti-skid-switch</object-name>
   <action> 
    <button>0</button>
      <binding>
        <command>property-toggle</command>
        <property>/controls/gear/antiskid</property>                
        <repeatable>false</repeatable>
      </binding>
   </action> 
  </animation> 
  
    <animation>
   <type>rotate</type>
   <object-name>light-land-toggle</object-name>
   <property>controls/lighting/landing-lights</property>
   <factor>30</factor> 
   <min>0</min>
   <max>1</max>   
   <center> 
    <x-m> 0.04688</x-m> 
    <y-m> 0.04546</y-m> 
    <z-m>-0.00114</z-m> 
   </center> 
   <axis> 
    <x>0</x> 
    <y>-1</y> 
    <z>0</z> 
   </axis> 
  </animation>

  <animation> 
   <type>pick</type> 
   <object-name>light-land-toggle</object-name>
   <action> 
    <button>0</button>
      <binding>
        <command>property-toggle</command>
        <property>controls/lighting/landing-lights</property>                
        <factor>30</factor> 
        <min>0</min> 
        <max>1</max>
      </binding>
   </action> 
 </animation>
 
   <animation> 
   <type>pick</type> 
   <object-name>light-land-toggle</object-name>
   <action> 
    <button>1</button>
      <binding>
        <command>property-toggle</command>
        <property>controls/lighting/taxi-light</property>                
        <factor>-30</factor> 
        <min>0</min> 
        <max>1</max>
      </binding>
   </action> 
 </animation>

  <animation>
   <type>rotate</type>
   <object-name>TO-LD-switch</object-name>
   <property>gear/gear/caster-angle-deg</property>
   <factor>50</factor> 
   <min>0</min>
   <max>1</max>   
   <center> 
    <x-m>0.04821</x-m> 
    <y-m>0.00014</y-m> 
    <z-m>0.00275</z-m> 
   </center> 
   <axis> 
    <x>0</x> 
    <y>1</y> 
    <z>0</z> 
   </axis> 
  </animation>

  <animation> 
   <type>pick</type> 
   <object-name>TO-LD-switch</object-name>
   <action> 
    <button>0</button>
      <binding>
        <command>property-multiply</command>
        <property>gear/gear/caster-angle-deg</property>                
        <factor>2</factor> 
        <min>0</min> 
        <max>1</max>
      </binding>
   </action> 
 </animation>

</PropertyList>
