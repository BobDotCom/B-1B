<?xml version="1.0"?>



<PropertyList>
	<path>ARN-118-tacan.ac</path>

	<animation>
		<type>textranslate</type>
		<object-name>drum-3</object-name>
		<property>instrumentation/tacan/frequencies/selected-channel[3]</property>
		<factor>-0.1</factor>
		<offset>1</offset>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>unit-knob</object-name>
		<property>instrumentation/tacan/frequencies/selected-channel[3]</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>0.0</dep>
			</entry>
			<entry>
				<ind>9.0</ind>
				<dep>-180.0</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>0.016</x-m>
			<y-m>0.009</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>unit-knob</object-name>
		<action>
			<button>0</button>
			<repeatable>true</repeatable>
			<interval-sec>0.2</interval-sec>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/tacan/frequencies/selected-channel[3]</property>
				<step>1</step>
				<min>0.0</min>
				<max>9.0</max>
			</binding>
		</action>
		<action>
			<button>1</button>
			<repeatable>true</repeatable>
			<interval-sec>0.2</interval-sec>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/tacan/frequencies/selected-channel[3]</property>
				<step>-1</step>
				<min>0.0</min>
				<max>9.0</max>
			</binding>
		</action>
</animation>

	<animation>
		<type>textranslate</type>
		<object-name>drum-2</object-name>
		<property>instrumentation/tacan/frequencies/selected-channel[2]</property>
		<factor>-0.1</factor>
		<offset>1</offset>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>textranslate</type>
		<object-name>drum-1</object-name>
		<property>instrumentation/tacan/frequencies/selected-channel[1]</property>
		<factor>-0.1</factor>
		<offset>1</offset>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>tenth-knob</object-name>
		<property>instrumentation/tacan/frequencies/selected-channel[12]</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>0.0</dep>
			</entry>
			<entry>
				<ind>12.0</ind>
				<dep>-240.0</dep>
			</entry>
	        </interpolation>
		<center>
			<x-m>0.016</x-m>
			<y-m>-0.050</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>tenth-knob</object-name>
		<action>
			<button>0</button>
			<repeatable>true</repeatable>
			<interval-sec>0.2</interval-sec>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/tacan/frequencies/selected-channel[12]</property>
				<step>1</step>
				<min>0.0</min>
				<max>12.0</max>
			</binding>
			<binding>
				<command>nasal</command>
				<script>b1b.tacan(1)</script>
		 </binding>
		</action>
		<action>
			<button>1</button>
			<repeatable>true</repeatable>
			<interval-sec>0.2</interval-sec>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/tacan/frequencies/selected-channel[12]</property>
				<step>-1</step>
				<min>0.0</min>
				<max>12.0</max>
			</binding>
			<binding>
				<command>nasal</command>
				<script>b1b.tacan(-1)</script>
		 </binding>
		</action>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>Y-sign</object-name>
		<condition>
			<equals>
				<property>instrumentation/tacan/frequencies/selected-channel[4]</property>
				<value>Y</value>
			</equals>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>X-sign</object-name>
		<condition>
			<equals>
				<property>instrumentation/tacan/frequencies/selected-channel[4]</property>
				<value>X</value>
			</equals>
		</condition>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>xy-knob</object-name>
		<action>
			<button>0</button>
			<repeatable>true</repeatable>
			<interval-sec>0.2</interval-sec>
			<binding>
				<command>nasal</command>
				<script>b1b.tacanxy()</script>
		        </binding>
		</action>
	</animation>


	<!-- lighting -->
	<animation>
		<object-name>mount</object-name>
		<type>material</type>
		<emission> 
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.44</red>
			<green>0.27</green>
			<blue>0.15</blue>
		</emission>
	</animation>

	<animation>
		<object-name>xy-knob</object-name>
		<type>material</type>
		<emission> 
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.35</red>
			<green>0.24</green>
			<blue>0.14</blue>
		</emission>
	</animation>

	<animation>
		<object-name>select-knob</object-name>
		<type>material</type>
		<emission> 
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.35</red>
			<green>0.24</green>
			<blue>0.14</blue>
		</emission>
	</animation>

	<animation>
		<type>material</type>
		<object-name>vol-knob</object-name>
		<object-name>unit-knob</object-name>
		<object-name>tenth-knob</object-name>
		<object-name>test-button</object-name>
		<object-name>test-lamp</object-name>
		<emission> 
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.35</red>
			<green>0.24</green>
			<blue>0.14</blue>
		</emission>
	</animation>

	<animation>
		<object-name>drum-1</object-name>
		<type>material</type>
		<emission> 
			<factor-prop>controls/lighting/instruments-norm</factor-prop>
			<red>0.60</red>
			<green>0.35</green>
			<blue>0.23</blue>
		</emission>
	</animation>

	<animation>
		<object-name>drum-2</object-name>
		<type>material</type>
		<emission> 
			<factor-prop>controls/lighting/instruments-norm</factor-prop>
			<red>0.60</red>
			<green>0.35</green>
			<blue>0.23</blue>
		</emission>
	</animation>

	<animation>
		<object-name>drum-3</object-name>
		<type>material</type>
		<emission> 
			<factor-prop>controls/lighting/instruments-norm</factor-prop>
			<red>0.60</red>
			<green>0.35</green>
			<blue>0.23</blue>
		</emission>
	</animation>

	<animation>
		<object-name>X-sign</object-name>
		<type>material</type>
		<emission> 
			<factor-prop>controls/lighting/instruments-norm</factor-prop>
			<red>0.60</red>
			<green>0.35</green>
			<blue>0.23</blue>
		</emission>
	</animation>

	<animation>
		<object-name>Y-sign</object-name>
		<type>material</type>
		<emission> 
			<factor-prop>controls/lighting/instruments-norm</factor-prop>
			<red>0.60</red>
			<green>0.35</green>
			<blue>0.23</blue>
		</emission>
	</animation>

	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5</max-m>
	</animation>

</PropertyList>
