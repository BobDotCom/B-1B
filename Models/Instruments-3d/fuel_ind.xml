 <animation>
	 <type>textranslate</type>
	 <object-name>fuel1</object-name>
	 <property>consumables/fuel/tank[2]/level-lbs</property>
	 <interpolation>
		 <entry>
			 <ind>0.0</ind>
			 <dep>0.0</dep>
		 </entry>
		 <entry>
			 <ind>35000.0</ind>
			 <dep>0.108</dep>
		 </entry>
	 </interpolation>
	 <axis>
		 <x>0.0</x>
		 <y>1.0</y>
		 <z>0.0</z>
	 </axis>
 </animation>

 <animation>
	 <type>material</type>
	 <object-name>fuel1</object-name>
	 <emission>
		 <red>0.9</red>
		 <green>0.5</green>
		 <blue>0.9</blue>
		 <factor-prop>controls/lighting/instruments-norm</factor-prop>
