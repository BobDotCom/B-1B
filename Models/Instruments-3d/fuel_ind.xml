<?xml version="1.0"?>
<!--
sweat by maggse
    -->
<PropertyList>

 <path>fuel_ind.ac</path>
 

 <!-- fuel indicators -->

 <animation>
	 <type>textranslate</type>
	 <object-name>fuel1</object-name>
	 <property>consumables/fuel/tank[2]/level-lbs</property>
	 <interpolation>
		 <entry>
			 <ind>0.0</ind>
			 <dep>0.0</dep>
		 </entry>
		 <entry>
			 <ind>35000.0</ind>
			 <dep>0.108</dep>
		 </entry>
	 </interpolation>
	 <axis>
		 <x>0.0</x>
		 <y>1.0</y>
		 <z>0.0</z>
	 </axis>
 </animation>
 <animation>
	 <type>material</type>
	 <object-name>fuel1</object-name>
	 <diffuse>
		<red>0.9</red>
	   <green>0.1</green>
	   <blue>0.2</blue>
	 </diffuse>
         <ambient>
	   <red>0.9</red>
	   <green>0.1</green>
	   <blue>0.2</blue>
	 </ambient>
	 <emission>
		 <red>0.9</red>
		 <green>0.1</green>
		 <blue>0.2</blue>
		 <factor-prop>controls/lighting/instruments-norm</factor-prop>
	</emission>
</animation>
 
